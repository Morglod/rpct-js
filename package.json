{
  "name": "rpct",
  "version": "0.1.1",
  "description": "rpc toolkit",
  "browser": "browser/rpct.min.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && npm run-script build-browser && npm run-script build-browser-min && npm run-script build-figma-min",
    "build-browser": "browserify lib/browser.js --standalone RPCT -o browser/rpct.js",
    "build-browser-min": "browserify -p tinyify lib/browser.js --standalone RPCT -o browser/rpct.min.js",
    "build-figma-min": "browserify -p tinyify lib/figma.js --standalone RPCT -o browser/figma.min.js"
  },
  "keywords": [
    "rpc",
    "toolkit",
    "network",
    "remote",
    "call",
    "figma-rpc",
    "fimga-plugin",
    "fimga-plugins",
    "fimga-plugin-rpc"
  ],
  "author": "morglod",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.5.0",
    "msgpack5": "^4.2.1",
    "tinyify": "^2.5.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@types/node": "^12.0.7",
    "@types/socket.io-client": "^1.4.32",
    "duplex-json-stream": "^1.0.1",
    "hyperid": "^2.0.2",
    "tsargs": "^1.3.3",
    "tsee": "^1.3.0"
  }
}
