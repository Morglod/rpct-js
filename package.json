{
  "name": "rpct",
  "version": "0.0.1",
  "description": "rpc toolkit",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && npm run-script browserify && npm run-script uglify",
    "browserify": "browserify lib/browser.js --standalone RPCT -o browser/rpct.js",
    "uglify": "browserify -g [ uglifyify ] lib/browser.js --standalone RPCT -o browser/rpct.min.js"
  },
  "keywords": [
    "rpc",
    "toolkit",
    "network",
    "remote",
    "call"
  ],
  "author": "morglod",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.5.0",
    "typescript": "^3.5.1",
    "uglifyify": "^5.0.2",
    "msgpack5": "^4.2.1"
  },
  "dependencies": {
    "@types/node": "^12.0.7",
    "@types/socket.io-client": "^1.4.32",
    "duplex-json-stream": "^1.0.1",
    "hyperid": "^2.0.2",
    "tsargs": "^1.3.3",
    "tsee": "^1.2.1"
  }
}
