    "msgpack5": "^4.2.1",
    "tinyify": "^2.5.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@types/node": "^12.0.7",
    "@types/socket.io-client": "^1.4.32",
    "duplex-json-stream": "^1.0.1",
    "hyperid": "^2.0.2",
    "tsargs": "^1.3.3",
    "tsee": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Morglod/rpct-js"
